Table brands {
  id int [pk, increment]
  name varchar [not null]
}

Table cpu_series {
  id int [pk, increment]
  name varchar [not null]
}

Table integrated_graphics {
  id int [pk, increment]
  name varchar [not null]
}

Table processors {
  id int [pk, increment]
  brand_id int [not null]
  name varchar [not null]
  series_id int [not null]
  socket_id int [not null]
  number_of_cores int [not null]
  number_of_threads int [not null]
  operating_frequency_mhz float [not null]
  turbo_frequency_mhz float [not null]
  thermal_design_power_w int [not null]
  cooling_device_included bool [not null]
  integrated_graphics_id int
  image_url varchar [not null]
}

Table drive_series {
  id int [pk, increment]
  name varchar [not null]
}

Table drive_types {
  id int [pk, increment]
  name varchar [not null]
}

Table drive_form_factors {
  id int [pk, increment]
  name varchar [not null]
}

Table drives {
  id int [pk, increment]
  brand_id int [not null]
  series_id int [not null]
  drive_type_id int [not null]
  form_factor_id int [not null]
  model varchar [not null]
  capacity_gb int [not null]
  max_read_mb_s int [not null]
  max_write_mb_s int [not null]
  interface_id int [not null]
  image_url varchar [not null]
}

Table gpu_chipset_manufacturers {
  id int [pk, increment]
  name varchar [not null]
}

Table gpu_memory_types {
  id int [pk, increment]
  name varchar [not null]
}

Table gpu_outputs {
  id int [pk, increment]
  graphics_card_id int [not null]
  output_id int [not null]
  quantity int [not null]
}

Table gpu_power_connectors {
  id int [pk, increment]
  graphics_card_id int [not null]
  power_connector_id int [not null]
  quantity int [not null]
}

Table graphics_cards {
  id int [pk, increment]
  brand_id int [not null]
  name varchar [not null]
  model varchar [not null]
  chipset_manufacturer_id int [not null]
  memory_type_id int [not null]
  memory_size_gb int [not null]
  thermal_design_power_w int [not null]
  slot_width int [not null]
  interface_id int [not null]
  image_url varchar [not null]
}

Table mb_chipset_manufacturers {
  id int [pk, increment]
  name varchar [not null]
}

Table mb_chipsets {
  id int [pk, increment]
  name varchar [not null]
  manufacturer_id int [not null]
}

Table mb_form_factors {
  id int [pk, increment]
  name varchar [not null]
}

Table mb_interfaces {
  id int [pk, increment]
  motherboard_id int [not null]
  interface_id int [not null]
  quantity int [not null]
  type varchar [not null]
}

Table mb_outputs {
  id int [pk, increment]
  motherboard_id int [not null]
  output_id int [not null]
  quantity int [not null]
}

Table motherboards {
  id int [pk, increment]
  brand_id int [not null]
  model varchar [not null]
  socket_id int [not null]
  chipset_id int [not null]
  form_factor_id int [not null]
  memory_type_id int [not null]
  max_memory_supported int [not null]
  memory_slots int [not null]
  image_url varchar [not null]
}

Table adapters {
  id int [pk, increment]
  name varchar [not null]
}

Table power_connector_connection_configurations {
  id int [pk, increment]
  power_connector_id int [not null]
  connection_configuration_id int [not null]
}

Table connection_configurations {
  id int [pk, increment]
  name varchar [not null]
}

Table energy_efficiencies {
  id int [pk, increment]
  name varchar [not null]
}

Table modularity {
  id int [pk, increment]
  name varchar [not null]
}

Table power_connectors {
  id int [pk, increment]
  name varchar [not null]
}

Table psu_series {
  id int [pk, increment]
  name varchar [not null]
}

Table psu_adapters {
  id int [pk, increment]
  power_supply_id int [not null]
  adapter_id int [not null]
  quantity int [not null]
}

Table psu_power_connectors {
  id int [pk, increment]
  power_supply_id int [not null]
  power_connector_id int [not null]
  cable_count int [not null]
  connector_count int [not null]
}

Table power_supplies {
  id int [pk, increment]
  brand_id int [not null]
  series_id int [not null]
  model varchar [not null]
  max_power_w int [not null]
  energy_efficiency_id int [not null]
  modularity_id int [not null]
  image_url varchar [not null]
}

Table ram_memory_types {
  id int [pk, increment]
  name varchar [not null]
}

Table ram_series {
  id int [pk, increment]
  name varchar [not null]
}

Table random_access_memory {
  id int [pk, increment]
  brand_id int [not null]
  series_id int [not null]
  model varchar [not null]
  memory_type_id int [not null]
  timing varchar [not null]
  cas_latency int [not null]
  voltage float [not null]
  transfer_rate_mb int [not null]
  speed int [not null]
  modules int [not null]
  module_capacity_gb int [not null]
  total_capacity_gb int [not null]
  image_url varchar [not null]
}

Table interfaces {
  id int [pk, increment]
  name varchar [not null]
  version float
}

Table outputs {
  id int [pk, increment]
  name varchar [not null]
}

Table sockets {
  id int [pk, increment]
  name varchar [not null]
}

